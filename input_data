  cfl    = 0.1 		!Courant-Friedrich-Levy
  tfinal = 10.		!final time
  nstepmax = 10     !max steps
  idiag  = 10000	!output period
  md = 2			!md : wave number x
  nd = 2			!nd : wave number y
  nx = 1000			!x number of points
  ny = 1000			!y number of points
  dimx = 10			!width
  dimy = 10			!height
$end
